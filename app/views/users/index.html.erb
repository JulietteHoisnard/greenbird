<div class="container">
  <div class="title-users">
    <h2>All users</h2>
  </div>
  <div class="search-container">
  </div>
  <div class="users-list">
    <% @users.each do |user| %>
    <div class="user-container">

      <%= cl_image_tag user.avatar_url, class: "avatar-profile",crop: :fill %>

    </div>
    <% end %>
  </div>
</div>